{
  "name": "workspaces-prefer-linking",
  "version": "1.0.0",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "name": "workspaces-prefer-linking",
      "version": "1.0.0",
      "workspaces": [
        "packages/*"
      ]
    },
    "node_modules/a": {
      "resolved": "packages/a",
      "link": true
    },
    "node_modules/abbrev": {
      "resolved": "packages/abbrev",
      "link": true
    },
    "packages/a": {
      "name": "a",
      "version": "1.0.0",
      "dependencies": {
        "abbrev": "*"
      }
    },
    "packages/abbrev": {
      "name": "abbrev",
      "version": "2.0.0"
    }
  },
  "dependencies": {
    "a": {
      "version": "file:packages/a"
    },
    "abbrev": {
      "version": "file:packages/abbrev"
    }
  }
}
